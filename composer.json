{
  "description": "didapptic app database",
  "license": [
    "MIT"
  ],
  "require": {
    "php": "7.4.*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zlib": "*",
    "ext-pcntl": "*",
    "doganoo/inotify": "1.0.1",
    "doganoo/php-algorithms": "0.0.24",
    "doganoo/php-util": "^0.3.16",
    "doganoo/backgrounder": "^3.0.0",
    "doganoo/simple-rbac": "1.5.0",
    "guzzlehttp/guzzle": "~6.0",
    "php-curl-class/php-curl-class": "*",
    "phpmailer/phpmailer": "6.*",
    "robmorgan/phinx": "0.10.6",
    "slim/slim": "^3.0",
    "symfony/http-foundation": "4.2.1",
    "tuupola/slim-basic-auth": "3.*",
    "twig/twig": "^2.0",
    "xobotyi/php-mime-type": "1.2.0",
    "php-di/php-di": "^6.0",
    "leafo/lessphp": "^0.5.0"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Didapptic\\": "src/",
      "DidappticTest\\": "test/"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^7",
    "phpstan/phpstan": "^0.12.25"
  }
}
